#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/daria.chiriac/Desktop/eventsme/eventsme-be/node_modules/.pnpm/typeorm@0.3.20_ts-node@10.9.2/node_modules/typeorm/node_modules:/Users/daria.chiriac/Desktop/eventsme/eventsme-be/node_modules/.pnpm/typeorm@0.3.20_ts-node@10.9.2/node_modules:/Users/daria.chiriac/Desktop/eventsme/eventsme-be/node_modules/.pnpm/node_modules:/Users/daria.chiriac/Desktop/eventsme/eventsme-be/node_modules:/Users/daria.chiriac/Desktop/eventsme/node_modules:/Users/daria.chiriac/Desktop/node_modules:/Users/daria.chiriac/node_modules:/Users/node_modules:/node_modules:/Users/daria.chiriac/Desktop/eventsme/eventsme-be/node_modules/typeorm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/daria.chiriac/Desktop/eventsme/eventsme-be/node_modules/.pnpm/typeorm@0.3.20_ts-node@10.9.2/node_modules/typeorm/node_modules:/Users/daria.chiriac/Desktop/eventsme/eventsme-be/node_modules/.pnpm/typeorm@0.3.20_ts-node@10.9.2/node_modules:/Users/daria.chiriac/Desktop/eventsme/eventsme-be/node_modules/.pnpm/node_modules:/Users/daria.chiriac/Desktop/eventsme/eventsme-be/node_modules:/Users/daria.chiriac/Desktop/eventsme/node_modules:/Users/daria.chiriac/Desktop/node_modules:/Users/daria.chiriac/node_modules:/Users/node_modules:/node_modules:/Users/daria.chiriac/Desktop/eventsme/eventsme-be/node_modules/typeorm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli.js" "$@"
else
  exec node  "$basedir/../typeorm/cli.js" "$@"
fi
